@@ -926,7 +926,7 @@ ucam: camera@36 {
 		port {
 			ucam_out: endpoint {
 				remote-endpoint = <&mipi_in_ucam>;
-				data-lanes = <1 2>;
+				data-lanes = <1 2 3 4>;
 			};
 		};
 	};
@@ -1144,7 +1144,7 @@ port@0 {
 			mipi_in_ucam: endpoint@0 {
 				reg = <0>;
 				remote-endpoint = <&ucam_out>;
-				data-lanes = <1 2>;
